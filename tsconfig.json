{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "module": "es2015",
        "moduleResolution": "node",
        "sourceMap": true,
        "target": "es5",
        "lib": [
            "dom",
            "es2015"
        ]
    },
    "filesGlob": [
        "**/*.ts",
        "!node_modules/**/*"
    ],
    "exclude": [
        "node_modules",
        "typings/main",
        "typings/main.d.ts"
    ],
    "compileOnSave": false,
    "atom": {
        "rewriteTsconfig": false
    },
    "ArcticTypescript": {
        "node_path": "/usr/local/bin/node"
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "files": [
        "src/app/app.component.ts",
        "src/app/app.module.ts",
        "src/app/main.ts",
        "src/declarations.d.ts",
        "src/pages/dashboard/dashboard.ts",
        "src/pages/detail-tabs/detail-tabs.ts",
        "src/pages/drill-detail/drill-detail.ts",
        "src/pages/drill-list/drill-list.ts",
        "src/pages/drill-youtube/drill-youtube.ts",
        "src/pages/home/home.ts",
        "src/pages/session-list/session-list.ts",
        "src/pages/settings/settings.ts",
        "src/pages/tabs/tabs.ts",
        "src/pages/timer/countdownTimer.ts",
        "src/pages/timer/timer.ts",
        "src/pages/timer/timer_new.ts",
        "src/providers/app-settings.ts",
        "src/providers/drill-data.ts",
        "src/providers/user-data.ts",
        "typings/angularjs/angular.d.ts",
        "typings/browser.d.ts",
        "typings/browser/ambient/cordova-plugin-drills/countdownTimer.ts",
        "typings/browser/ambient/cordova-plugin-drills/index.d.ts",
        "typings/globals/cordova/index.d.ts",
        "typings/globals/jquery/index.d.ts",
        "typings/index.d.ts",
        "typings/main.d.ts",
        "typings/main/ambient/cordova-plugin-drills/index.d.ts",
        "typings/tsd.d.ts"
    ]
}